

{
    "main":{
        "tag":"div",
        "class":"items"


        },
      "einhorn":{
        "tag":"div",
        "class":"e",
        "inner":[{
            "tag":"img",
            "class":"bild",
            "src":"dasdickeinhorn.jpg"
          }]

      },




    "item":{
        "tag":"div",
        "class":"item",
        "inner":[{

              "tag":"buttun",
              "class":"fa fa-close",
              "onclick":"%click%"

            },
            {
                "tag":"div",
                "class":"datum",
                "inner":"%datum%"
            },
            {
                "tag":"div",
                "class":"name",
                "inner":"%name%"
            },
            {
                "tag":"div",
                "class":"text",
                "inner":"%text%"
            },{
                "tag":"div",
                "class":"erstdatum",
                "inner":"%erstdatum%"
            },{
              "tag":"div",
              "class":"fa fa-arrow-up",
              "onclick":"%upclick%"
            }

        ]
    },

    "neuesItem":{
        "tag":"div",
        "class":"new_item",
        "inner":[
            {
                "tag":"div",
                "class":"name1"
            },
            {
                "tag":"div",
                "class":"text1",
                "inner":[

                    {
                        "tag":"form",
                        "inner":[



                            {
                                "tag":"input",
                                "class":"neuesItem",
                                "placeholder":"Beschreibung"
                            },{
                                "tag":"input",
                                "class":"datumi",
                                "placeholder":"Datum"
                            },
                            {
                                "tag":"input",
                                "class":"button",
                                "type":"submit",
                                "value":"Speichern"


                            }
                        ],
                        "onsubmit":"%onsubmit%"
                    }

                ]
            }

        ]
    }

}
